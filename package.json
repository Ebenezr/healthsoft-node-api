{
  "name": "health-soft-node",
  "version": "1.0.0",
  "description": "nodejs server",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json src/index.ts",
    "prisma": "prisma",
    "build": "tsc",
    "prestart": "npx prisma migrate dev --name init && yarn seed ",
    "start": "yarn build && node dist/index.js",
    "seed": "ts-node ./prisma/seed.ts --accept-data-loss"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "nodejs",
    "express",
    "prisma",
    "postgresql",
    "docker"
  ],
  "author": "Ebenezr",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^7.7.7",
    "faker": "^6.6.6",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "response-time": "^2.3.2",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/node": "^18.11.13",
    "jest": "^29.3.1",
    "prisma": "^4.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
